            width: 500px;
        }

        .cardMargin {
            border-width: 10px;
            border-style: solid;
            border-color: darkgoldenrod;
        }

        /* el shorthand margin modifica el margen de la capa de borde en forma general, es decir, en las 4 direcciones posibles y con el valor indicado por el valor de la propiedad */
        .cardMargin01 {
            border-width: 10px;
            border-style: solid;
            border-color: crimson;

            margin: 50px;
        }

        /* las propiedades que modifican el margen de cada una de las direcciones de la capa de margen son para el borde superior margin-top, para el borde derecho margin-right, para el borde inferior margin-bottom, para el borde izquierdo margin-left */
        .cardMargin02 {
            border-width: 10px;
            border-style: solid;
            border-color: crimson;

            margin-top: 20px;
            margin-right: 50px;
            margin-bottom: 30px;
            margin-left: 10px;
        }

        /* el shorthand margin puede recibir 4 valores que se corresponden con las 4 propiedades que representan las direcciones de la capa de borde. Los valores que recibe se corresponden con margin: top right bottom left */
        .cardMargin03 {
            border-width: 10px;
            border-style: solid;
            border-color: crimson;

